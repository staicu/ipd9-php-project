{% extends "index.html.twig" %}
{% block container %}
    <div class="container">
        <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Phone</th>
                    
                </tr>
            </thead>
            <tbody>
               
                {% for s in userList %}
                    <tr><td>{{s.id}}</td>
                        <td>{{s.name}}</td>
                        <td>{{s.phone}}</td>
                    </tr>   
                {% endfor %}
            </tbody>
        </table>
    </div>
{% endblock %}